% function bin_image = classify_organs(bin_image)
%     classes = {'left kidney', 'right kidney','left rib','right rib','spine',...
%         'liver', 'bowel loops', 'muscles', 'stomach'};
%     
%     properties = ["Area","Centroid","Perimeter","Bounding Box"];
%     s = regionprops(bin_image,'centroid')
% end
classes = {'left kidney', 'right kidney','spine','liver', 'bowel loops', 'muscles', 'stomach'};


for i=1:7
    name=classes{1,i}
    init_ref_props(name)
end





function obj = init_ref_props(name,prop1,prop2)
    obj.name = name
    obj.value = sqrt(prop1^2+prop2^2)
end